<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.michael.spec" auto-import="false">
    <class name="com.michael.spec.domain.Youth" table="SPEC_YOUTH">
        <id name="id" column="id" length="40">
            <generator class="uuid"/>
        </id>

        <property name="name" column="NAME" type="string" length="40" not-null="true"/>
        <property name="sex" column="SEX" type="string" length="40"/>
        <property name="nation" column="NATION" type="string" length="40"/>
        <property name="education" column="EDUCATION" type="string" length="40"/>
        <property name="birthday" column="BIRTHDAY" type="timestamp"/>
        <property name="age" column="AGE" type="int"/>
        <property name="mobile" column="MOBILE" type="string" length="20"/>
        <property name="tel" column="TEL" type="string" length="20"/>
        <property name="qq" column="QQ" type="string" length="20"/>
        <property name="email" column="EMAIL" type="string" length="100"/>
        <property name="picture" column="PICTURE" type="string" length="40"/>
        <property name="address" column="ADDRESS" type="string" length="100"/>
        <property name="ownerId" column="OWNER_ID" type="string" length="40"/>
        <property name="ownerName" column="OWNER_NAME" type="string" length="40"/>
        <property name="orgId" column="ORG_ID" type="string" length="40" not-null="true"/>
        <property name="orgName" column="ORG_NAME" type="string" length="40"/>
        <property name="state" column="Y_STATE" type="string" length="40" not-null="true"/>
        <property name="content" column="Y_CONTENT" type="string" length="2000"/>
        <property name="helpTimes" column="HELP_TIMES" type="int"/>
        <property name="lastHelpDate" column="LAST_HELP_DATE" type="date"/>
        <property name="reason" column="REASON" type="string" length="200"/>
        <property name="interest" column="INTEREST" type="string" length="50"/>

        <property name="description" column="DESCRIPTION" type="string" length="2000"/>
        <property name="creatorId" column="CREATOR_ID" type="string" length="40" update="false"/>
        <property name="creatorName" column="CREATOR_NAME" type="string" length="40"/>
        <property name="createdDatetime" column="CREATED_DATETIME" type="timestamp" update="false"/>
        <property name="modifiedDatetime" column="MODIFIED_DATETIME" type="timestamp" update="false"/>
    </class>
</hibernate-mapping>
